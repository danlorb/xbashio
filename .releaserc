{
  "branches": [
    {
      "name": "main",
      "channel": "stable"
    },
    {
      "name": "next",
      "channel": "next",
      "prerelease": "next"
    }
  ],
  "ci": true,
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    [
      "@saithodev/semantic-release-gitea",
      {
        "assets": [
          {
            "path": "dist/**/*.zip",
            "label": "Application"
          }
        ]
      }
    ]
  ]
}
